{
  "schemaVersion": 1,
  "id": "supplemental_patches",
  "version": "0.1.0-beta",
  "name": "Supplemental Patches",
  "description": "Additional patches to Euphoria Patches providing better support for many modded blocks from popular mods.",
  "authors": [
    "lemon41625"
  ],
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.jedlimlx.supplemental_patches.SupplementalPatches"
      }
    ]
  },
  "accessWidener" : "supplemental_patches.accesswidener",
  "mixins": [
    "supplemental_patches.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}